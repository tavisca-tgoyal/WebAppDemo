stage('Checkout'){
    steps{
        git credentialsId: 'tavisca-tgoyal', url: 'https://github.com/tavisca-tgoyal/WebAppDemo.git', branch: 'master'
    }
}

stage('Restore Packages'){
    steps{
        bat "dotnet restore"
    }
}

stage('Clean') {
 steps {
  bat "dotnet clean"
 }
}

stage('Build') {
 steps {
  bat "dotnet build --configuration Release"
 }
}

stage('Pack') {
 steps {
  bat "dotnet pack --no-build --output nupkgs"
 }
}

